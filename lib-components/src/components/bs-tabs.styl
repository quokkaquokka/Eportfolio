bs-tabs, .bs-tabs
  display block

  nav>div.nav.nav-tabs
    margin-bottom 0

  &>div.card
    border-top 0
    border-top-left-radius 0
    border-top-right-radius 0
    margin-top 0

  &.card>.card-body>div
  &>select
    display inline-block
    margin-bottom 10px

  &>p
    display inline-block
    margin-right 10px
  a
    font-size 30px
  div
    font-size 15px
